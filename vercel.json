{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/signin$",
      "dest": "/signin.html"
    },
    {
      "src": "^/signup$",
      "dest": "/signup.html"
    },
    {
      "src": "^/todo$",
      "dest": "/todo.html"
    },
    {
      "src": "^/account-information$",
      "dest": "/account-information.html"
    },
    {
      "src": "^/change-password$",
      "dest": "/change-password.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
